<!-- AB HIER DIE STYLES DEFINIEREN -->


<Style id="Reich-n">
		<ListStyle><maxSnippetLines>0</maxSnippetLines></ListStyle>
		<LineStyle>
			<color>ff0000ff</color>
			<width>5</width>
		</LineStyle>
		<PolyStyle>
			<fill>0</fill>		
		</PolyStyle>
		<BalloonStyle>
        <!-- a background color for the balloon -->
        <bgColor>FFD5FCFD</bgColor>
        <!-- styling of the balloon text -->
<text><![CDATA[<font face="Mason" color="#CC0000" size="+3"><a title="Im Wiki-Aventurica öffnen" href="https://www.wiki-aventurica.de/wiki/$[Link]">$[name]</a></font>
<br/><br/>
$[description]]]></text>
        <textColor>FF000000</textColor>
      </BalloonStyle>
	</Style>
