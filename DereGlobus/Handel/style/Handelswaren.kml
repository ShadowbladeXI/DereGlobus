<!-- AB HIER DIE STYLES DEFINIEREN -->






<Style id="Zinn-n">
		<ListStyle>
			<maxSnippetLines>0</maxSnippetLines>
		</ListStyle>
		<LabelStyle>
			<scale>0</scale>
			<color>00ffffff</color>
		</LabelStyle>

		<IconStyle>
			<scale>0.9</scale>
			<color>ffffffff</color>
			<colorMode>normal</colorMode>
			<Icon>
				<href>https://www.dereglobus.orkenspalter.de/public/DereGlobus/Handel/media/icons/bodenschaetze/Zinn.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
		<BalloonStyle>
        <!-- a background color for the balloon -->
        <bgColor>FFD5FCFD</bgColor>
        <!-- styling of the balloon text -->
        <text><![CDATA[
      <font face="Mason" color="#CC0000" size="+3">$[name]</font>
      <br/><br/>
      $[description]
      ]]></text>
        <textColor>FF000000</textColor>
      </BalloonStyle>
	</Style>


