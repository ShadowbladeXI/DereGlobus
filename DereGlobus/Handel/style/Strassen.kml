	<Style id="Reichsstrasse-n">
		<BalloonStyle>
        <!-- a background color for the balloon -->
        <bgColor>FFD5FCFD</bgColor>
        <!-- styling of the balloon text -->
<text><![CDATA[<font face="Mason" color="#CC0000" size="+3"><a title="Im Wiki-Aventurica öffnen" href="https://www.wiki-aventurica.de/wiki/$[Link]">$[name]</a></font>
<br/><br/>
$[description]]]></text>
        <textColor>FF000000</textColor>
      </BalloonStyle>
      <ListStyle>
         <maxSnippetLines>0</maxSnippetLines>
      </ListStyle>
		<IconStyle>
			<scale>1.1</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
		</IconStyle>
		<LineStyle>
			<color>7fffffff</color>
			<width>4</width>
		</LineStyle>
	</Style>

	
	
