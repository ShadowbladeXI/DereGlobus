<!-- So, ab hier werden die einzelnen Styles definiert -->
	<!-- Mögliche Styles sind 
		<IconStyle>...</IconStyle>
		<LabelStyle>...</LabelStyle>
		<LineStyle>...</LineStyle>
		<PolyStyle>...</PolyStyle>
		<BalloonStyle>...</BalloonStyle>
		<ListStyle>...</ListStyle>
	 -->

	<Style id="staerke_1-n">
		<BalloonStyle>
                  <!-- a background color for the balloon -->
                  <bgColor>FFD5FCFD</bgColor>
                  <!-- styling of the balloon text -->
                  <text><![CDATA[
                    <font face="Mason" color="#CC0000" size="+3"><a title="Im Wiki-Aventurica öffnen" href="https://www.wiki-aventurica.de/wiki/$[Link]">$[name]</a></font>
                    <br/><br/>
                    $[description]
                    ]]></text>
                  <textColor>FF000000</textColor>
                </BalloonStyle>
                <ListStyle>
                  <maxSnippetLines>0</maxSnippetLines>
                </ListStyle>
		<IconStyle>
	          <scale>1.1</scale>
		  <Icon>
		    <href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
		  </Icon>
		  <hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
		</IconStyle>
		<LineStyle>
		  <color>FFA51CD6</color>
		  <width>1</width>
		</LineStyle>
	</Style>
