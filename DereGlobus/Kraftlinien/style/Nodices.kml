        <Style id="nodix_1-n">
		<ListStyle><maxSnippetLines>0</maxSnippetLines></ListStyle>
		<LabelStyle>
		  <scale>0</scale>
		  <color>00FFFFFF</color>
		</LabelStyle>
		<IconStyle>
		  <scale>1.0</scale>
		  <color>FFFFFFFF</color>
		  <colorMode>normal</colorMode>
		  <Icon>
		    <href>https://www.dereglobus.orkenspalter.de/public/DereGlobus/Kraftlinien/media/icons/Magischer_Ort3.png</href>
		  </Icon>
		  <hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
		<BalloonStyle>
                  <!-- a background color for the balloon -->
                  <bgColor>FFD5FCFD</bgColor>
                  <!-- styling of the balloon text -->
                  <text><![CDATA[
                    <font face="Mason" color="#CC0000" size="+3"><a title="Im Wiki-Aventurica öffnen" href="https://www.wiki-aventurica.de/wiki/$[Link]">$[name]</a></font>
                    <br/><br/>
                    $[description]
                    ]]></text>
                  <textColor>FF000000</textColor>
                </BalloonStyle>
	</Style>
