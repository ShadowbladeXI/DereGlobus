<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Menue-Style</name>

<StyleMap id="menue">
		<Pair>
			<key>normal</key>
			<styleUrl>#menue-n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#menue-h</styleUrl>
		</Pair>
	</StyleMap>

    <Style id="menue-n">
		<ListStyle>
		  	<ItemIcon>
				<href>https://www.dereglobus.orkenspalter.de/public/DereGlobus/Menue/media/Menue.png</href>	
			</ItemIcon>
			<maxSnippetLines>0</maxSnippetLines>
		</ListStyle>
		<IconStyle>
			<scale>0.9</scale>
			<color>ffffffff</color>
			<colorMode>normal</colorMode>
			<Icon>
				<href>https://www.dereglobus.orkenspalter.de/public/DereGlobus/Menue/media/Menue.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
	  </IconStyle>
      <BalloonStyle>
        <!-- a background color for the balloon -->
        <bgColor>FF1C1A1A</bgColor>
        <!-- styling of the balloon text -->
        <text><![CDATA[
      <font face="Calibri" color="#366698" size="+3">$[name]</font>
      <br/><br/>
	<font color="#bbbbbb">$[description]</font>
      ]]></text>
        <textColor>FF000000</textColor>
      </BalloonStyle>
    </Style>

