<Document>

	<StyleMap id="Stadtteil">
		<Pair>
			<key>normal</key>
			<styleUrl>#Stadtteil-n</styleUrl>
		</Pair>
		<Pair>
			<key>highlight</key>
			<styleUrl>#Stadtteil-h</styleUrl>
		</Pair>
	</StyleMap>
	
	
	<Style id="Stadtteil-n">
		<BalloonStyle>
        <text><![CDATA[
      <font face="Mason" color="#CC0000" size="+3"><a title="Im Wiki-Aventurica öffnen" href="http://www.wiki-aventurica.de/wiki/$[Link]">$[name]</a></font>
      <br/><br/>
      $[description]
      ]]></text>
<bgColor>ffd5fcfd</bgColor>
</BalloonStyle>
		<ListStyle>
			<maxSnippetLines>0</maxSnippetLines>
		</ListStyle>
		<LineStyle>
			<color>03000055</color>
			<width>0.1</width>
		</LineStyle>
		<PolyStyle>
			<color>03000055</color>
		</PolyStyle>
		<IconStyle>
			<scale>0</scale>
			<Icon>
				<href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
			</Icon>
			<hotSpot x="20" y="2" xunits="pixels" yunits="pixels"/>
		</IconStyle>
		<LabelStyle>
			<scale>0</scale>
			<color>a8ffffff</color>
		</LabelStyle>
	</Style>
	
	<Style id="Stadtteil-h">
